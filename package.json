{
  "name": "@fastly/next-compute-js-server-12.3.0",
  "version": "0.1.0",
  "license": "MIT",
  "description": "Next.js 12.3.0 Runtime for Fastly Compute@Edge",
  "main": "./dist/server/index.js",
  "types": "./dist/server/index.d.ts",
  "scripts": {
    "prepack": "npm run build",
    "clean": "rimraf dist",
    "build": "npm run test && npm run clean && npm run build-server",
    "build-server": "npm run build-server:compile",
    "build-server:compile": "tsc --build tsconfig.server.json",
    "test": "npm run test-server",
    "test-server": "c8 mocha -n no-warnings=86652 -n loader=ts-node/esm --extension ts --require test/mocha_bootstrap.js test/testHelpers.ts test/**/*.test.ts"
  },
  "peerDependencies": {
    "next": "12.3.0",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
  },
  "dependencies": {
    "@fastly/compute-js-static-publish": "^4.2.0",
    "@fastly/http-compute-js": "^0.4.0",
    "@fastly/js-compute": "^1.11.1"
  },
  "devDependencies": {
    "@types/assert": "^1.5.6",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.4",
    "@types/react": "^18.0.34",
    "@types/react-dom": "^18.0.11",
    "@types/sinon": "^10.0.13",
    "assert": "^2.0.0",
    "c8": "^7.13.0",
    "mocha": "^10.2.0",
    "next": "12.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^4.1.2",
    "sinon": "^14.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "files": [
    "dist/src/**/*.js",
    "dist/src/**/*.js.map",
    "dist/src/**/*.d.ts",
    "LICENSE",
    "README.md",
    "SECURITY.md"
  ]
}
